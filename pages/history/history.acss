.staffList {
  display: block;
  width: 80%;
  border-radius: 10px;
  margin: 0 5vw 10vw 5vw;
  background-color: white;
  padding: 5vw;
  box-shadow: 0px 10px 10px rgba(227, 231, 234, 1);
}
.place {
  margin-left: 5vw;
  display: list-item;
  color: #7F7F7F;
  font-size: 0.3rem;
}
.msg {
  margin: 2vw 0 2vw 5vw;
  padding-bottom: 2vw;
}
.timesty {
  margin: 5vw 0 3vw 5vw;
  font-size: 0.3rem;
  color: #7F7F7F;
  font-weight: 200;
}